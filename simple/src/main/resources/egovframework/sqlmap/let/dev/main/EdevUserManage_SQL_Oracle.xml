<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="EDevUserManage">
	<parameterMap id="eDevUser" class="egovframework.com.dev.service.EDevUser">
		<parameter property="userId" />
		<parameter property="userName" />
		<parameter property="userAge" />
	</parameterMap>
	<insert id="EDevUserManageDAO.putUserInfo" parameterMap="eDevUser">
INSERT INTO JINDEV_USER.TEST_USER (ID, NAME, AGE, CREATE_DATE)
  VALUES ('$userId$', '$userName$', $userAge$, SYSDATE)
</insert>
</sqlMap>