<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="EDevBBSManage">
	<resultMap id="getBullList" class="egovframework.com.dev.service.EDevBBS">
		<result property="boardId" column="BOARDID"/>
		<result property="boardNm" column="BOARDNAME"/>
		<result property="bullId" column="BULLID"/>
		<result property="bullTitle" column="BULLTITLE"/>
		<result property="bullContent" column="CONTENT"/>
		<result property="bullRegId" column="REGID"/>
		<result property="bullRegNm" column="REGNAME"/>
		<result property="bullRegDate" column="REGDATE"/>
	</resultMap>
	<resultMap id="getBoardNmList" class="egovframework.com.dev.service.EDevBBS">
		<result property="boardId" column="BOARDID"/>
		<result property="boardNm" column="BOARDNAME"/>
	</resultMap>
	<parameterMap id="bbsId" class="egovframework.com.dev.service.EDevBBS">
		<parameter property="boardId" />
	</parameterMap>
	<parameterMap id="eDevBBS" class="egovframework.com.dev.service.EDevBBS">
		<parameter property="boardId" />
		<parameter property="boardNm" />
		<parameter property="bullId" />
		<parameter property="bullTitle" />
		<parameter property="bullContent" />
		<parameter property="bullRegId" />
		<parameter property="bullRegNm" />
		<parameter property="bullRegDate" />
	</parameterMap>
	<select id="EDevBBSManageDAO.getBoardNmList" resultMap="getBoardNmList">
		SELECT DISTINCT BOARDID, BOARDNAME
		FROM JINDEV_USER.TBB_TEST_BOARD
	</select>
	<select id="EDevBBSManageDAO.getBullList" resultClass="egovframework.com.dev.service.EDevBBS" resultMap="getBullList">
		SELECT BOARDID, BOARDNAME, BULLID, BULLTITLE, CONTENT, REGID, REGNAME, REGDATE
		FROM JINDEV_USER.TBB_TEST_BOARD
	</select>
	<insert id="EDevBBSManageDAO.putNoticeBull" parameterClass="egovframework.com.dev.service.EDevBBS">
		INSERT INTO JINDEV_USER.TBB_TEST_BOARD (BOARDID, BOARDNAME, BULLID, BULLTITLE, CONTENT, REGID, REGNAME, REGDATE)
		VALUES (#boardId#, #boardNm#, #bullId#, #bullTitle#, EMPTY_CLOB(), #bullRegId#, #bullRegNm#, sysdate)
	</insert>
</sqlMap>
